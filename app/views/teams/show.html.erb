<h1><%= @team.name %></h1>
<h4>League: <%= link_to @league.name, @league %>
  <h4>User: <%= link_to @user.username, @user %> </h4>
<% if @team.players.empty?%>
  <%= "You're team hasn't been drafted yet" %>
<% else %>
Players: 
<ul>
    <% @team.players.each do |player| %>
      <li> <%= player.name %> </li>
      <% end %>
  <% end %>
</ul>


<% if current_user == @user %>
  <%= button_to "Edit Team Name", edit_league_team_path(@league, @team), method: :get %>
<% end %>
